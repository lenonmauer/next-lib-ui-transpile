{
  "name": "lenon-ds",
  "main": "dist/index.js",
  "sideEffects": false,
  "version": "0.0.16",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn clean && yarn transpile && yarn emit-types",
    "transpile": "babel src --out-dir dist --extensions '.ts,.tsx,.jsx,.js' --source-maps inline",
    "emit-types": "tsc --emitDeclarationOnly --declaration --outDir dist",
    "clean": "rimraf dist",
    "prepublishOnly": "yarn build",
    "p": "npm version patch && npm publish"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.0",
    "@babel/plugin-transform-modules-commonjs": "^7.21.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.0",
    "@types/react": "^18.0.25",
    "clsx": "^1.2.1",
    "react": "^17",
    "rimraf": "^4.4.0",
    "styled-jsx": "^5.1.2",
    "typescript": "^4.9.5"
  }
}
